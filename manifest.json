{
  "name": "Cookie 管理器",
  "version": "5.0",
  "description": "允许创建多个 cookie 保存项。",
  "permissions": [
    "cookies",
    "storage",
    "tabs",
    "webRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "action": {
    "default_title": "Backup & restore your cookies!",
    "default_popup": "popup.html"
  },
  "manifest_version": 3
}